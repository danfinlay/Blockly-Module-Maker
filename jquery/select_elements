
//Select by Element Type:
Blockly.Language.jquery_select_elements = {
  helpUrl: helpUrl,
  init: function() {
    this.setColour(65);
    this.appendDummyInput()
        .appendTitle("Element Type")
        .appendTitle(new Blockly.FieldTextInput("body"), "el_type");
    this.setInputsInline(true);
    this.setOutput(true, 'jquery_selector');
    this.setTooltip('');
  }
};
Blockly.JavaScript.jquery_select_elements = function() {
  var text_el_type = this.getTitleValue('el_type');
  var code = '"'+text_el_type+'"';
  // TODO: Change ORDER_NONE to the correct strength.
  return [code, Blockly.JavaScript.ORDER_ATOMIC];
};


