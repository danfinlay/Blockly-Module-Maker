//.after()
Blockly.Language.jquery_after = {
  helpUrl: helpUrl,
  init: function() {
    this.setColour(330);
    this.appendValueInput("newHtml")
        .setCheck("String")
        .appendTitle("After");
    this.setOutput(true, 'jquery_action');
    this.setTooltip('Add provided content after each selected element.');
  }
};
Blockly.JavaScript.jquery_after = function() {
  var value_newhtml = Blockly.JavaScript.valueToCode(this, 'newHtml', Blockly.JavaScript.ORDER_NONE);
  var code = '.after('+value_newhtml+')';
  return [code, Blockly.JavaScript.ORDER_ATOMIC];
};

